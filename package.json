    "@types/lodash": "^4.14.149",
    "@shopify/jest-koa-mocks": "^2.1.10",
    "@types/http-errors": "^1.6.3",
    "@types/koa": "^2.11.0",
    "@types/koa-route": "^3.2.4",
    "@types/pg": "^7.11.2",
    "@types/uuid": "^3.4.6",
    "@types/faker": "^4.1.8",
    "@types/hapi__joi": "^16.0.4",
    "@types/koa-router": "^7.0.42",
    "@types/marked": "^0.7.2",
    "jest": "^24.9.0",
