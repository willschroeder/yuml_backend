{
  "name": "yuml_backend",
  "version": "1.0.0",
  "description": "yuml backend",
  "main": "index.js",
  "scripts": {
    "test": "npx jest",
    "tsc": "tsc",
    "dev": "DEBUG=yuml:* ts-node-dev --respawn --transpileOnly ./src/main.ts",
    "build": "rm -rf ./build/* && tsc",
    "prod": "DEBUG=yuml:* node ./build/main.js",
    "clean": "rm -rf ./build/*",
    "lint": "npx eslint 'src/**/*.ts' --fix",
    "migrate": "DEBUG=yuml:* ts-node-dev ./src/main.ts command migrate",
    "drop": "DEBUG=yuml:* ts-node-dev ./src/main.ts command drop",
    "testcmd": "DEBUG=yuml:* ts-node-dev ./src/main.ts command testcmd"
  },
  "author": "will schroeder",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^16.1.8",
    "compromise": "^11.14.3",
    "debug": "^4.1.1",
    "execa": "^3.4.0",
    "faker": "^4.1.0",
    "http-errors": "^1.7.3",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa-route": "^3.2.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.15",
    "marked": "^4.0.10",
    "numeric-quantity": "^0.5.1",
    "pg": "^7.14.0",
    "postgrator": "^3.11.0",
    "pug": "^2.0.4",
    "query-string": "^6.9.0",
    "uuid": "^3.3.3",
    "@types/jest": "^24.0.23",
